# .env.example for Secrets Service

# Port the service will run on
PORT=3002

# !! IMPORTANT !! Secret key for AES encryption (32 bytes for AES-256 recommended)
# Generate a secure random key. Example using OpenSSL: openssl rand -base64 32
SECRET_VAULT_KEY=your-secure-32-byte-secret-key-here

# Database connection string (if using a database)
# e.g., POSTGRES_URL=postgresql://user:password@host:port/database

# Node environment
NODE_ENV=development
